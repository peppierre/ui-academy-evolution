<article class="vehicle-detail">
    <header>
        <a href="#/logout">Log out</a>
    </header>
    <section>
        <section>
            <img src="http://res.cloudinary.com/hgzhd1stm/image/upload/c_scale/{{images.[0].key}}"/>
            <h2>{{name}}</h2>
            <div>{{variants.[0].price.amount}} {{variants.[0].price.currency}}</div>
            <section class="parking-zone">
                In your parking zone <input type="number" value="{{parkingzone}}" max="5" min="0" />
            </section>
        </section>
        <section>
            <table>
                {{#each properties}}
                    <tr>
                        <th>{{key}}</th>
                        <td>{{value}}</td>
                    </tr>
                {{/each}}
            </table>
        </section>
        <div><a href="#/oldtimers">Go back</a></div>
    </section>
</article>
